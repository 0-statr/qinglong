{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "build-back": "tsc",
    "start-back": "nodemon",
    "pm2": "npm run build && node build/app.js",
    "postinstall": "umi generate tmp",
